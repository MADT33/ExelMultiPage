<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified" xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="raizen.cgral.controller.View" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<IconTabBar selectedKey="__filter0" id="bar0">
							<items>
								<IconTabFilter text="CeBe" id="filter0">
									<content>
										<core:Fragment fragmentName="raizen.cgral.fragments.CeBe" type="XML"/>
									</content>
								</IconTabFilter>
								<IconTabFilter text="CeCo" id="filter1">
									<content>
										<core:Fragment fragmentName="raizen.cgral.fragments.CeCo" type="XML"/>
									</content>
								</IconTabFilter>
								<IconTabFilter text="Cuenta BS" id="filter2">
									<content>
										<core:Fragment fragmentName="raizen.cgral.fragments.CuentaBS" type="XML"/>
									</content>
								</IconTabFilter>
								<IconTabFilter text="Cuenta PL" id="filter3">
									<content>
										<core:Fragment fragmentName="raizen.cgral.fragments.CuentaPL" type="XML"/>
									</content>
								</IconTabFilter>
								<IconTabFilter text="Grupo de Articulo" id="filter4">
									<content>
										<core:Fragment fragmentName="raizen.cgral.fragments.GrupArt" type="XML"/>
									</content>
								</IconTabFilter>
								<IconTabFilter text="Mapeo Grupo balance" id="filter5">
									<content>
										<core:Fragment fragmentName="raizen.cgral.fragments.MapGrupBal" type="XML"/>
									</content>
								</IconTabFilter>
								<IconTabFilter text="Mapeo Grupo PyL" id="filter6">
									<content>
										<core:Fragment fragmentName="raizen.cgral.fragments.MapGrupPyL" type="XML"/>
									</content>
								</IconTabFilter>
							</items>
						</IconTabBar>
					</content>
					<footer>
						<OverflowToolbar>
							<ToolbarSpacer/>
							<u:FileUploader id="fileUploader" name="myFileUpload" buttonOnly="true" change="onUploadFile" buttonText="{i18n>upload}" icon="sap-icon://upload"/>
							
							<HBox width="43%" id="__hbox0" alignItems="End" alignContent="End">
							    <items>
							        <CheckBox text="{i18n>Opcion1}" id="idbox1"/>
							        <CheckBox text="{i18n>Opcion2}" id="idbox2"/>
							        <CheckBox text="{i18n>Opcion3}" id="idbox3"/>
							    </items>
							</HBox>
							<Button text="{i18n>guardar}" icon="sap-icon://save" press="onGuardar"/>
						<content/>
						</OverflowToolbar>
					</footer>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>