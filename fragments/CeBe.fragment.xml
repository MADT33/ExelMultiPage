<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified" xmlns="sap.m"
	controllerName="raizen.cgral.controller.View">
	<Table id="id-table" growing="true" growingScrollToLoad="true" growingThreshold="3000" items="{localModel>/CEBESet}" >
		<columns>
			<Column>
				<Label text="{i18n>cebeN1}"/>
			</Column>
			<Column>
				<Label text="{i18n>cebeN1Descr}"/>
			</Column>
			<Column>
				<Label text="{i18n>cebeN2}"/>
			</Column>
			<Column>
				<Label text="{i18n>cebeN2Descr}"/>
			</Column>
			<Column>
				<Label text="{i18n>cebe}"/>
			</Column>
			<Column>
				<Label text="{i18n>cebeDescr}"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem type="Active">
				<cells>
					<Text text="{localModel>CebeN1}"/>
					<Text text="{localModel>CebeN1Desc}"/>
					<Text text="{localModel>CebeN2}"/>
					<Text text="{localModel>CebeN2Desc}"/>
					<Text text="{localModel>Cebe}"/>
					<Text text="{localModel>CebeDesc}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>